CREATE TABLE Teachers (
    TeacherId INT PRIMARY KEY,
    FirstName VARCHAR(50),
    LastName VARCHAR(50),
    DateOfBirth DATE,
    Gender VARCHAR(10),
    SubjectTaught VARCHAR(50)
)

INSERT INTO Teachers (TeacherId, FirstName, LastName, DateOfBirth, Gender, SubjectTaught)
