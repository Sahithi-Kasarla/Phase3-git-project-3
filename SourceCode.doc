create database Schoolmanagement
use Schoolmanagement

CREATE TABLE Students (
    StudentId INT PRIMARY KEY,
    FirstName VARCHAR(50),
    LastName VARCHAR(50),
    DateOfBirth DATE,
    Gender VARCHAR(10)
)

INSERT INTO Students (StudentId, FirstName, LastName, DateOfBirth, Gender)
